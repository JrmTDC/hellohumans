{
     "extends": "@adonisjs/tsconfig/tsconfig.app.json",
     "compilerOptions": {
          "rootDir": "./",
          "outDir": "./.output",
          "strict": true,
          "baseUrl": "./",
          "paths": {
               "#sockets/*": [
                    "app/sockets/*"
               ],
               "#start/*": [
                    "start/*"
               ],
               "#config/*": [
                    "config/*"
               ]
          }
     },
     "include": ["**/*.ts", "bin/**/*.ts"]
}
